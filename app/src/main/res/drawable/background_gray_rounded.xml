<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- default state -->
    <item android:state_enabled="true" android:state_pressed="false" android:state_focused="false">
        <shape>
            <solid android:color="@color/text_light_grey"/>
            <corners android:radius="12dp"/>
            <stroke android:width="@dimen/stroke_width" android:color="@color/text_light_grey"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/text_light_grey"/>
            <corners android:radius="12dp" />
        </shape>
    </item>
</layer-list>