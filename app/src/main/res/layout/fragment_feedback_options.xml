<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fragment_feedback_options_replace_button">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingStart="16dp"
        android:gravity="center_vertical"
        android:id="@+id/feedback_token_given">

        <TextView
            android:id="@+id/large_volume"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:background="@drawable/background_purple_oval"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:text="Token Given"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textSize="17sp"
            android:textColor="#444A59"
            android:paddingRight="30dp"
            android:paddingEnd="30dp"
            android:paddingLeft="14dp"
            android:paddingStart="14dp"/>

        <ImageView
            android:layout_height="24dp"
            android:layout_width="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_chevron_right"
            android:scaleType="fitCenter"
            android:padding="4dp"
            android:background="@android:color/transparent"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="center"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="#D8DEEB "/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingStart="16dp"
        android:gravity="center_vertical"
        android:id="@+id/feedback_token_pending">

        <TextView
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:background="@drawable/background_purple_oval"
            android:gravity="center"
            android:textSize="12sp" />

        <TextView
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:text="Token Pending"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textSize="17sp"
            android:textColor="#444A59"
            android:paddingRight="30dp"
            android:paddingEnd="30dp"
            android:paddingLeft="14dp"
            android:paddingStart="14dp"/>

        <ImageView
            android:layout_height="24dp"
            android:layout_width="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_chevron_right"
            android:scaleType="fitCenter"
            android:padding="4dp"
            android:background="@android:color/transparent"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="center"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="#D8DEEB "/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingStart="16dp"
        android:paddingRight="16dp"
        android:gravity="center_vertical"
        android:id="@+id/feedback_negotiation_pending">

        <TextView
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:background="@drawable/background_green_circle"
            android:gravity="center"
            android:textColor="#00B289" />

        <TextView
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:text="Negotiation Pending"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textSize="17sp"
            android:textColor="#444A59"
            android:paddingRight="30dp"
            android:paddingEnd="30dp"
            android:paddingLeft="14dp"
            android:paddingStart="14dp"/>

        <ImageView
            android:layout_height="24dp"
            android:layout_width="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_chevron_right"
            android:scaleType="fitCenter"
            android:padding="4dp"
            android:background="@android:color/transparent"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="center"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="#D8DEEB "/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingStart="16dp"
        android:paddingRight="16dp"
        android:gravity="center_vertical"
        android:id="@+id/feedback_decision_pending">

        <TextView
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:background="@drawable/background_green_circle"
            android:gravity="center"
            android:textColor="#00B289"
            android:textSize="12sp" />

        <TextView
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:text="Decision Pending"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textSize="17sp"
            android:textColor="#444A59"
            android:paddingRight="30dp"
            android:paddingEnd="30dp"
            android:paddingLeft="14dp"
            android:paddingStart="14dp"/>

        <ImageView
            android:layout_height="24dp"
            android:layout_width="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_chevron_right"
            android:scaleType="fitCenter"
            android:padding="4dp"
            android:background="@android:color/transparent"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="center"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="#D8DEEB "/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingStart="16dp"
        android:gravity="center_vertical"
        android:id="@+id/feedback_negotiation_unsuccessful">

        <TextView
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:background="@drawable/background_orange_circle"
            android:gravity="center"
            android:textColor="#FF7143"
            android:textSize="12sp" />

        <TextView
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:text="Negotiation Unsucessful"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textSize="17sp"
            android:textColor="#444A59"
            android:paddingRight="30dp"
            android:paddingEnd="30dp"
            android:paddingLeft="14dp"
            android:paddingStart="14dp"/>

        <ImageView
            android:layout_height="24dp"
            android:layout_width="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_chevron_right"
            android:scaleType="fitCenter"
            android:padding="4dp"
            android:background="@android:color/transparent"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="center"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="#D8DEEB "/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingStart="16dp"
        android:gravity="center_vertical"
        android:id="@+id/feedback_car_quality_issue">

        <TextView
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:background="@drawable/background_orange_circle"
            android:gravity="center"
            android:textColor="#FF7143"
            android:textSize="12sp" />

        <TextView
            android:layout_width="0sp"
            android:layout_height="wrap_content"
            android:text="Car Quality Issue"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textSize="17sp"
            android:textColor="#444A59"
            android:paddingRight="30dp"
            android:paddingEnd="30dp"
            android:paddingLeft="14dp"
            android:paddingStart="14dp"/>

        <ImageView
            android:layout_height="24dp"
            android:layout_width="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_chevron_right"
            android:scaleType="fitCenter"
            android:padding="4dp"
            android:background="@android:color/transparent"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="center"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="#D8DEEB "/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingStart="16dp"
        android:gravity="center_vertical"
        android:id="@+id/feedback_others">

        <TextView
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:gravity="center"
            android:textColor="#FF7143"
            android:textSize="12sp" />

        <TextView
            android:layout_width="0px"
            android:layout_height="wrap_content"
            android:text="Others"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textSize="17sp"
            android:textColor="#444A59"
            android:paddingRight="30dp"
            android:paddingEnd="30dp"
            android:paddingLeft="14dp"
            android:paddingStart="14dp"/>

        <ImageView
            android:layout_height="24dp"
            android:layout_width="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_chevron_right"
            android:scaleType="fitCenter"
            android:padding="4dp"
            android:background="@android:color/transparent"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="center"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="#D8DEEB "/>

</LinearLayout>
   

